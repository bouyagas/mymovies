{
  "name": "mymovies",
  "version": "1.0.0",
  "description": "This is an App the saves my favorites movies",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "heroku-prebuild": "bash ./scripts/deployment_react.sh",
    "heroku-postbuild": "bash ./scripts/deployment_cleanup.sh",
    "key": "ssh-add",
    "watch": "webpack -d --progress --watch",
    "start": "node index.js",
    "db": "psql mymovies",
    "dev": "nodemon",
    "heroku": "git push heroku master",
    "heroku-schema": "heroku pg:psql --app edtronics < db/schema.sql",
    "heroku-seed": "heroku pg:psql --app edtronics < db/seed.sql",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bouyagas/mymovies.git"
  },
  "author": "Mohamed Gassama",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bouyagas/mymovies/issues"
  },
  "homepage": "https://github.com/bouyagas/mymovies#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-strict-mode": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "chai": "^4.1.0",
    "css-loader": "^0.26.1",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "html-webpack-template": "^6.0.0",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "style-loader": "^0.13.1",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0"
  },
